            "post": {
                "tags": [
                    "Accounts"
                ],
                "summary": "Creates an account with the specified name.",
                "description": "Currently, this endpoint only supports the creation of current accounts.\r\n            \r\nAccountName must: not be null, only white space, only contain letters, numbers and the following special characters: - ,.\r\n            \r\nThis endpoint uses the X-Request-Id as a duplicate check and to ensure the request is idempotent.",
